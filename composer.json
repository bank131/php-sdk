{
  "name": "131/php-sdk",
  "type": "library",
  "description": "Bank131 PHP SDK",
  "keywords": [
    "bank131",
    "131",
    "api",
    "sdk",
    "payment processing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bank 131 team",
      "homepage": "https://131.ru/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "~7.0",
    "psr/log": ">=1.1",
    "psr/http-message": ">=1.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=8.0",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "Bank131\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bank131\\SDK\\Tests\\": "tests/"
    }
  }
}
